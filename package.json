{
  "name": "electron-template",
  "version": "1.0.0",
  "description": "An Electron application with React, NestJS and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --sourcemap",
    "debug": "electron-vite dev --sourcemap --remote-debugging-port=9222",
    "postinstall": "electron-builder install-app-deps",
    "build": "pnpm typecheck && electron-vite build",
    "build:unpack": "pnpm build && electron-builder --config ./electron-builder.json --dir",
    "build:win": "pnpm build && electron-builder --config ./electron-builder.json --win",
    "build:mac": "pnpm build && electron-builder --config ./electron-builder.json --mac",
    "build:linux": "pnpm build && electron-builder --config ./electron-builder.json --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "better-sqlite3": "^11.9.1",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/material": "^7.0.0",
    "@optimize-lodash/rollup-plugin": "^5.0.2",
    "@swc/core": "^1.11.13",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.13",
    "@types/node": "^18.19.68",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.1.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^6.28.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-checker": "^0.9.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.2",
    "zustand": "^4.5.6",
    "zustand-x": "^3.0.4"
  },
  "engines": {
    "node": ">=18.6.0"
  },
  "packageManager": "pnpm@10.7.0+sha512.6b865ad4b62a1d9842b61d674a393903b871d9244954f652b8842c2b553c72176b278f64c463e52d40fff8aba385c235c8c9ecf5cc7de4fd78b8bb6d49633ab6"
}
